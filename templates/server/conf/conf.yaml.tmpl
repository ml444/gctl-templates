debug: true
pprof_cfg:
  enable_pprof: false
  pprof_addr: ":5060"
http_cfg:
  enable_http: true
  enable_swagger: true
  http_addr: ":5050"
  timeout: 2000   # 单位毫秒
  tls_cert_file: ""
  tls_key_file: ""
  router_path_prefix: ""
  router_strict_slash: true
  router_skip_clean: false
  router_use_encoded_path: false
grpc_cfg:
  enable_grpc: false
  enable_health: false
  grpc_addr: ":5040"
  tls_cert_file: ""
  tls_key_file: ""
db_cfg:
  db_uri: "root:password@tcp(192.168.1.11:3306)/db_name?charset=utf8&parseTime=True&loc=Local"
  # 连接池中每个连接的最大生存时间，单位秒。
  conn_max_life_time: 57600
  # 连接池中每个连接的最大空闲时间，单位秒。
  conn_max_idle_time: 7200
  # 连接池中允许同时打开的最大连接数
  max_open_conns: 100
  # 连接池中允许存在的最大空闲连接数
  max_idle_conns: 10
